{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "es2015",
    "moduleResolution": "bundler",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["src/**/*"]
}
